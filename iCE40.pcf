set_io fdti_dtr 3
set_io fdti_dsr 2
set_io fdti_dcd 1


#ftdi master, fpga slave
set_io fdti_clk 70
set_io ftdi_din 68
set_io ftdi_dout 67
set_io ftdi_ss 71



#latch pulse (shift->output)
#set_io latch 90

#set_io q_prime 95


